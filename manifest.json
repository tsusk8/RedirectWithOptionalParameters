{
    "name": "Sample",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting", "storage"],
    "options_page": "options.html",
    "description": "Sample Chrome Extension",
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["*://*/item/*"],
        "js": [
            "main.js"
        ]
    }]
}